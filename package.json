{
  "name": "startrail-subgraph",
  "version": "0.1.0",
  "scripts": {
    "create": "graph create --node https://api.thegraph.com/deploy/ startbahn/startrail-rinkeby-release",
    "create-local": "graph create --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-release",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "npm run codegen && graph deploy --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ startbahn/startrail-rinkeby-release",
    "deploy-local": "graph deploy --ipfs http://localhost:5001 --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-release"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
