{
  "name": "startrail-subgraph",
  "version": "1.0.0",
  "scripts": {
    "create-local-develop": "graph create --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-develop",
    "create-local-release": "graph create --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-release",
    "create-gke-develop": "graph create --node https://subgraph.startbahn.jp/admin/ startbahn/startrail-rinkeby-develop",
    "create-gke-release": "graph create --node https://subgraph.startbahn.jp/admin/ startbahn/startrail-rinkeby-release",
    "create-gke-master": "graph create --node http://subgraph.startrail.io/admin/ startbahn/startrail",
    "create": "graph create --node https://api.thegraph.com/deploy/ startbahn/startrail-rinkeby-release",
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy-local-release": "graph deploy --ipfs http://localhost:5001 --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-release",
    "deploy-local-develop": "graph deploy --ipfs http://localhost:5001 --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-develop",
    "deploy-gke-develop": "graph deploy --ipfs https://subgraph.startbahn.jp/ipfs/ --node http://subgraph.startbahn.jp/admin/ startbahn/startrail-rinkeby-develop",
    "deploy-gke-release": "graph deploy --ipfs https://subgraph.startbahn.jp/ipfs/ --node http://subgraph.startbahn.jp/admin/ startbahn/startrail-rinkeby-release",
    "deploy-gke-master": "graph deploy --ipfs http://subgraph.startrail.io/ipfs/ --node http://subgraph.startrail.io/admin/ startbahn/startrail",
    "deploy-develop": "graph deploy --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ startbahn/startrail-rinkeby-develop --access-token $TOKEN",
    "deploy-release": "graph deploy --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ startbahn/startrail-rinkeby-release --access-token $TOKEN",
    "deploy-master": "graph deploy --debug --ipfs https://api.thegraph.com/ipfs/ --node https://api.thegraph.com/deploy/ startbahn/startrail --access-token $TOKEN",
    "remove-local-release": "graph remove --ipfs http://localhost:5001 --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-release",
    "remove-local-develop": "graph remove --ipfs http://localhost:5001 --node http://127.0.0.1:8020 startbahn/startrail-rinkeby-develop"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "^0.18.0",
    "@graphprotocol/graph-ts": "^0.18.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "truffle": "^5.0.4",
    "truffle-contract": "^4.0.5",
    "truffle-hdwallet-provider": "^1.0.4"
  }
}
